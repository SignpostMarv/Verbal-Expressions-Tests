{
    "name": "signpostmarv/verbal-expressions-tests",
    "description": "Language-agnostic test definitions for VerbalExpressions implementations",
    "license": "MIT",
    "authors": [
        {
            "name": "SignpostMarv",
            "email": "me@signpostmarv.name"
        }
    ],
    "autoload": {
        "psr-4": {
            "SignpostMarv\\VerbalExpressionsTests\\Generator\\": "generator/php/"
        }
    },
    "bin": [
        "bin/verbal-expressions-tests-generator.php"
    ],
    "config": {
        "optimize-autoloader": true
    },
    "keywords": [
        "regular expressions",
        "regex",
        "verbal expressions",
        "tests"
    ],
    "suggests": {
        "verbalexpressions/php-verbal-expressions": "Composer install would suggest you're wanting to test the PHP implementation"
    },
    "require": {
        "php": ">=5.3",
        "justinrainbow/json-schema": "^1.6",
        "nikic/php-parser": "^2.0"
    }
}
